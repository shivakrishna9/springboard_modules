/**
 * Implements hook_uninstall().
 */
function salesforce_ngoc_uninstall(){
  // Remove user_to_batch_upload salesforce mapping.
  $user_mapping = salesforce_mapping_load('user_to_batch_upload');
  if (!empty($user_mapping)) {
    entity_delete('salesforce_mapping', $user_mapping->salesforce_mapping_id);
  }
  // Remove default donation mapping provided by salesforce_ngoc.
  salesforce_genmap_delete_map(-1, 'salesforce_donation');
}

/**
 * Creates the sbp_created_via field.
 *
 * Used to track what action created the user.
 */
function salesforce_ngoc_create_user_created_via_field() {
  $success = FALSE;
  $created_via = array(
    'label' => t('Created Via'),
    'field_name' => 'sbp_created_via',
    'type' => 'text',
    'widget' => array(
      'type' => 'text_textfield',
    ),
    'display' => array(
      'default' => array(
        'label' => 'hidden',
        'type' => 'hidden',
      ),
      'teaser' => array(
        'label' => 'hidden',
        'type' => 'hidden',
      ),
    ),
  );

  $field_exists = field_info_field('sbp_created_via');

  if (empty($field_exists)) {
    field_create_field($created_via);
  }

  $instance_exists = field_info_instance('user', 'sbp_created_via', 'user');
  if (empty($instance_exists)) {
    $instance = array(
      'entity_type' => 'user',
      'bundle' => 'user',
    );
    $instance = array_merge($created_via, $instance);
    field_create_instance($instance);
  }

  $field_group = field_group_load_field_group('group_profile_admin', 'user', 'user', 'form');
  $table = 'field_group';

  // If the group exists make sure its not disabled.
  if ($field_group) {
    $field_group->children[] = 'sbp_created_via';
    $success = ctools_export_crud_save($table, $field_group);
  }
  return $success;
}

