<?php

/**
 * @file
 * Install hooks for the Salesforce NGOC module.
 */

/**
 * Implements hook_uninstall().
 */
function salesforce_ngoc_install() {
  // Remove default Drupal user to Contact mapping provided by Springboard.
  $user_to_contact_mapping = salesforce_mapping_load('user_to_contact');
  if ($user_to_contact_mapping !== FALSE) {
    entity_delete('salesforce_mapping', $user_to_contact_mapping->salesforce_mapping_id);
  }

  // @TODO This also appears in salesforce_npsp_uninstall(). Should we require
  // that salesforce_npsp module be uninstalled instead?
  // Remove NPSP Salesforce mapping for fundraiser sustainers series.
  $sustainers_series_mapping = salesforce_mapping_load('fundraiser_sustainers_series_npsp');
  if (!empty($sustainers_series_mapping)) {
    entity_delete('salesforce_mapping', $sustainers_series_mapping->salesforce_mapping_id);
  }

  // Remove default donation mapping provided by Springboard.
  salesforce_genmap_delete_map(-1, 'salesforce_donation');

  // Create default NGOC Donation mapping.
  $donation_mapping_result = salesforce_ngoc_create_donation_mapping();
  if ($donation_mapping_result !== FALSE) {
    drupal_set_message(t('Donations have been mapped to NGOC Batch Upload object.'));
  }

  // Create default Drupal user mapping.
  $user_mapping_result = salesforce_ngoc_create_user_mapping();
  if ($user_mapping_result !== FALSE) {
    drupal_set_message(t('Drupal user entity has been mapped to NGOC Batch Upload object.'));
  }

  // Add profile field to track which module created a user.
  $field_creating_result = salesforce_ngoc_create_user_created_via_field();
  if ($user_mapping_result !== FALSE) {
    drupal_set_message(t('Drupal user profile field sbp_created_via has been created.'));
  }

  // Create default parent object data structure. This is used when determining
  // which queue items have un-sync'd parents and thus should be put in the
  // 'hold' queue rather than the 'new' queue until the parent has been
  // processed.
  $salesforce_ngoc_default_parent_objects = array(
    'sb_Actions_Taken__c' => array(
      'Contact' => 'user',
    ),
    'Social_Share__c' => array(
      'Opportunity' => 'donation',
      'Contact' => 'user',
    ),
    'Donation_Upsell__c' => array(
      'Opportunity' => 'donation',
    ),
    'P2P_Personal_Campaign__c' => array(
      'Contact' => 'user',
    ),
    'Sustainers_Upgrade__c' => array(
      'Contact' => 'user',
    ),
  );
  variable_set('salesforce_ngoc_parent_objects', $salesforce_ngoc_default_parent_objects);
}

/**
 * Implements hook_requirements().
 */
function salesforce_ngoc_requirements($phase) {
  $requirements = array();
  // Ensure translations don't break at install time.
  $t = get_t();
  if ($phase == 'install' || $phase == 'runtime') {
    $requirements['salesforce_gnoc'] = array(
      'title' => $t('Salesforce NGOC'),
      'description' => $t('The Salesforce NGOC module requires that the Salesforce NPSP module is disabled.'),
      'value' => $t('NPSP Disabled'),
      'severtiy' => REQUIREMENT_OK,
    );
    if (module_exists('salesforce_npsp')) {
      $requirements['salesforce_gnoc']['value'] = 'NPSP Enabled';
      $requirements['salesforce_gnoc']['severity'] = REQUIREMENT_ERROR;
    }
  }
  return $requirements;
}

